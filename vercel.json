{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "_site" }
    },
    {
      "src": "Gemfile",
      "use": "@vercel/ruby"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/_site/$1"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
