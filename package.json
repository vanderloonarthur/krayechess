{
  "name": "kraye",
  "private": true,
  "version": "4.24.0",
  "description": "Minimal Mistakes 2 column Jekyll theme.",
  "repository": {
    "type": "git",
    "url": "git://github.com/mmistakes/minimal-mistakes.git"
  },
  "dependencies": {
    "@vercel/fun": "1.1.0",
    "acorn": "^8.12.0",
    "ajv": "^6.12.6",
    "ansi-styles": "^3.2.1",
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "debug": "^4.3.5",
    "eslint-scope": "^8.0.1",
    "espree": "^10.0.1",
    "find-up": "^5.0.0",
    "glob": "^7.1.7",
    "globals": "^14.0.0",
    "has-flag": "^3.0.0",
    "import-fresh": "^3.3.0",
    "jquery": "^3.7.1",
    "lodash": "^4.17.21",
    "minimal-mistakes": "^0.0.1-security",
    "minimatch": "^3.1.2",
    "mkdirp": "^1.0.4",
    "natural-compare": "^1.4.0",
    "node-libs-browser": "^2.2.1",
    "node-releases": "^2.0.14",
    "resolve": "^1.22.8",
    "semver": "^5.7.2",
    "source-map": "^0.6.1",
    "supports-color": "^5.5.0",
    "uglifyjs": "^2.4.11",
    "validate-npm-package-license": "^3.0.4",
    "webpack-merge": "^5.10.0"
  },
  "keywords": [
    "jekyll",
    "theme",
    "minimal"
  ],
  "author": "Michael Rose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmistakes/minimal-mistakes/issues"
  },
  "homepage": "https://mmistakes.github.io/minimal-mistakes/",
  "engines": {
    "node": ">=20.x"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/runtime-corejs3": "^7.24.7",
    "babel-loader": "^9.1.3",
    "chokidar": "^3.6.0",
    "core-js-pure": "^3.37.1",
    "eslint": "^9.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-xo": "^1.0.0",
    "eslint-plugin-yarn-internal": "^0.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "susy": "^3.0.8",
    "uglify-js": "^3.13.6",
    "vercel": "^34.2.7",
    "webpack": "^5.36.2",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "uglify": "uglifyjs assets/js/vendor/jquery/jquery-3.6.0.js assets/js/plugins/*.js assets/js/_main.js -c -m -o assets/js/main.min.js",
    "add-banner": "node banner.js",
    "watch:js": "onchange 'assets/js/**/*.js' -e 'assets/js/main.min.js' -- yarn run build:js",
    "build:js": "yarn run uglify && yarn run add-banner",
    "build": "yarn run lint && yarn run compile",
    "vercel-build": "yarn run build && cp -r build/ _site/",
    "vercel": "vercel --prod",
    "lint": "eslint .",
    "compile": "webpack --config webpack.config.js"
  },
  "main": ".eslintrc.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "resolutions": {
    "crypto-browserify/diffie-hellman": "^5.0.0",
    "node-libs-browser/events": "^3.0.0"
  },
  "packageManager": "yarn@4.3.0"
}
